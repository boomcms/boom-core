{
    "name": "boomcms/boom-core",
    "description": "Core classes for BoomCMS",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-imagick": "*",
        "ext-zip": "*",
        "mpratt/embera": "1.9.*",
        "genphrase/genphrase": "1.*",
        "bordoni/phpass": "dev-main",
        "thujohn/rss": "~1.0",
        "intervention/imagecache": "2.*",
        "laravel/framework": "5.6.*",
        "rych/bytesize": "1.0.*",
        "laravelcollective/html": "5.5.*",
        "lootils/geo": "^0.1.1",
        "caxy/php-htmldiff": "^0.1.3",
        "doctrine/dbal": "2.5.*",
        "smalot/pdfparser": "*",
        "phpoffice/phpword": "0.13.*",
        "symfony/process": "4.4.*",
        "php-ffmpeg/php-ffmpeg": "^0.19.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "1.*",
        "codeclimate/php-test-reporter": "dev-master",
        "symfony/dom-crawler": "4.4.*",
        "laravel/browser-kit-testing": "4.2.*",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "BoomCMS\\": "src/BoomCMS/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoomCMS\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
